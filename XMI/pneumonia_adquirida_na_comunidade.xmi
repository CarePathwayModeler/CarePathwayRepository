  <element xsi:type="pathwayMetamodel:Information" id="17096" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="Discutir com chefia" description="&lt;p>&lt;span style=&quot;font-size:16px&quot;>Discutir conduta com chefia devido a prov&amp;aacute;vel interna&amp;ccedil;&amp;atilde;o.&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:16px&quot;>Em caso de interna&amp;ccedil;&amp;atilde;o:&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:16px&quot;>a) Prescrever Unasyn 100mg/kg/dia&amp;nbsp;de&amp;nbsp;6/6h EV se n&amp;atilde;o houver uso de ATB pr&amp;eacute;vio&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:16px&quot;>b) Prescrever Cetriaxona 50-100mg/kg/dia&amp;nbsp;EV se houver falha terapeutica com antibi&amp;oacute;tico oral pr&amp;eacute;vio.&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:16px&quot;>c) Considerar uso de Oxacilina 100-200mg/kg/dia em casos de PNM lobar/extensa&amp;nbsp;ou com derrame pleural.&amp;nbsp;&lt;/span>&lt;/p>" isTerminal="true" inputSequences="//@sequence.10"/>
  <element xsi:type="pathwayMetamodel:Prescription" id="17097" type="Receita" type_verbose="Receita" url="" name="Receita" description="&lt;p>&lt;span style=&quot;font-size:16px&quot;>Considerar alta com antibi&amp;oacute;tico oral e retorno em 48h para reavalia&amp;ccedil;&amp;atilde;o. &lt;/span>&lt;/p>" isTerminal="true" inputSequences="//@sequence.9">
    <item id="4593" url="" name="Amoxicilina + Clavulanato 400+57 mg/ml ___________ FR" med_code="-1" presentation="-" access="1" period="0" period_unit="DD" frequency="0" frequency_unit="DD" orientations="Dar ___ ml de 12/12h por 10 dias (conservar na geladeira)."/>
    <item id="4594" url="" name="Ceftriaxona 1g ________________________fr" med_code="-1" presentation="-" access="5" period="0" period_unit="DD" frequency="0" frequency_unit="DD" orientations="Aplicar 1 ampola IM, 1x por dia por 3 dias. Reavaliar ap&#xf3;s 48h."/>

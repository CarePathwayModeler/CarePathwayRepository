  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20343" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Procurar outras causas" description="Procurar outras causas da dor tor&#xe1;cica n&#xe3;o isqu&#xea;mica" inputSequences="//@sequence.27 //@sequence.28" outputSequences="//@sequence.29 //@sequence.30 //@sequence.31 //@sequence.32 //@sequence.33 //@sequence.34 //@sequence.35">
    <questions text="Pneumonia: Tosse, febre, dor tor&#xe1;cica em pontada, de forte intensidade, ao tossir ou respirar profundamente" url="" id="10311">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_pneumonia" id="19931" weight="1" value="false"/>
    </questions>
    <questions text="TEP/Pneumot&#xf3;rax: Dispneia s&#xfa;bita, com dor tor&#xe1;cica em pontada, de forte &#xA;intensidade, ao tossir ou respirar profundamente, podendo haver hemoptise, taquipneia, taquicardia" url="" id="10312">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_tep_pneumotorax" id="19932" weight="1" value="false"/>
    </questions>
    <questions text="Dissec&#xe7;&#xe3;o: Dor tor&#xe1;cica s&#xfa;bita, lancinante, de forte intensidade, com irradia&#xe7;&#xe3;o para dorso. Diferen&#xe7;a de pulso nos membros superiores contralaterais ou diferen&#xe7;a entre os pulsos superiores/inferiores. Diferen&#xe7;a de 20mmHg na PAS dos membros superiores" url="" id="10313">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_disseccao" id="19933" weight="1" value="false"/>
    </questions>
    <questions text="Pericardite: Dor precordial s&#xfa;bita, cont&#xed;nua ou intermitente, que piora com a inspira&#xe7;&#xe3;o profunda e melhora com o tronco inclinado pra frente, piorando ao deitar." url="" id="10314">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_pericardite" id="19934" weight="1" value="false"/>
    </questions>
    <questions text="DRGE/Espasmo esofagiano: Paciente com dor tor&#xe1;cica em queima&#xe7;&#xe3;o, ou intensa em c&#xf3;lica, associada a azia, regurgita&#xe7;&#xe3;o e/ou sensa&#xe7;&#xe3;o de entalo" url="" id="10315">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_drge_espasmo_esofagiano" id="19935" weight="1" value="false"/>
    </questions>
    <questions text="Dor m&#xfa;sculo-esquel&#xe9;tica: Dor que piora com a digitopress&#xe3;o na parede tor&#xe1;cica, com a movimenta&#xe7;&#xe3;o. Pode haver hist&#xf3;ria de trauma ou esfor&#xe7;o f&#xed;sico extenuante." url="" id="10316">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_cauasas_dor_musculo_esqueletica" id="19936" weight="1" value="false"/>
    </questions>
    <questions text="Dor psicog&#xea;nica: Paciente jovem, sem comorbidades, com hist&#xf3;ria de s&#xed;ndrome do p&#xe2;nico, referindo dor tor&#xe1;cica difusa, imprecisa, com sensa&#xe7;&#xe3;o de morte. Diagn&#xf3;stico de exclus&#xe3;o" url="" id="10317">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="outras_causas_dor_psicogenica" id="19937" weight="1" value="false"/>
    </questions>
  </element>

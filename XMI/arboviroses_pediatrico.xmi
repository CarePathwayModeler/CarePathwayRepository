<pathwayMetamodel:Pathway xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pathwayMetamodel="http://www.example.org/pathwayMetamodel" id="3382" url="" name="Arboviroses Pedi&#xe1;trico" code="H061">
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20403" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Avaliar tipo de arbovirose" description="&lt;table border=&quot;1&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot; dir=&quot;ltr&quot;>&#xA;&#x9;&lt;tbody>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Sinais/Sintomas&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Dengue&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Zika&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Chikunguya&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Febre (dura&amp;ccedil;&amp;atilde;o)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&amp;nbsp; Acima de 38&amp;ordm;C (4 a 7 dias)&amp;nbsp;&amp;nbsp;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&amp;nbsp; Sem febre ou subfebril 38&amp;ordm;C (1-2 dias subfebril)&amp;nbsp;&amp;nbsp;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Febre alta &amp;gt; 38&amp;ordm; (2-3 dias)&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Manchas na pele (Frequ&amp;ecirc;ncia)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>A partir do 4&amp;ordm; dia&lt;br />&#xA;&#x9;&#x9;&#x9;(30-50% dos casos)&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Surge no 1&amp;ordm; ou no 2&amp;ordm; dia (90-100% dos casos)&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Surge 2-5 dia&lt;br />&#xA;&#x9;&#x9;&#x9;(50% dos casos)&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Dor nos m&amp;uacute;sculos (Frequ&amp;ecirc;ncia)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>+++/+++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>++/+++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>+/+++&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Dor na articula&amp;ccedil;&amp;atilde;o (Frequ&amp;ecirc;ncia)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>+/+++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>++/+++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>+++/+++&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Intensidade da dor articular&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve/Moderada&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Moderada/Intensa&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Edema da articula&amp;ccedil;&amp;atilde;o&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Raro&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Frequente e leve intensidade&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&amp;nbsp; Frequente e de moderada a intensa&amp;nbsp;&amp;nbsp;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Conjuntivite&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Raro&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>50-90% dos casos&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>30%&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>&amp;nbsp; Cefal&amp;eacute;ia (Frequ&amp;ecirc;ncia e Intensidade)&amp;nbsp;&amp;nbsp;&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>+++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>++&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>++&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Prurido&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Moderada/Intensa&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Hipertrofia ganglionar (Frequ&amp;ecirc;ncia)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Intensa&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Moderada&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Discrasia hemorr&amp;aacute;gica (Frequ&amp;ecirc;ncia)&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Moderada&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Ausente&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Leve&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>&lt;strong>Acometimento neurol&amp;oacute;gico&lt;/strong>&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Raro&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Mais frequente que Dengue e Chikunguya&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>Raro (predominante em Neonatos)&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&lt;/tbody>&#xA;&lt;/table>" isInitial="true" outputSequences="//@sequence.0 //@sequence.13 //@sequence.19">
    <questions text="Escolha a arbovirose que se encaixa com o quadro do paciente:" url="" id="10325">
      <answers xsi:type="pathwayMetamodel:Choice" name="tipo_arbovirose" id="19983" weight="1" value="0.0">
        <option description="Dengue" weight="1"/>
        <option description="Zika" weight="2"/>
        <option description="Chikunguya" weight="3"/>
      </answers>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20404" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="[Dengue] Verifica Sinais de Alarme" description="" inputSequences="//@sequence.0" outputSequences="//@sequence.1 //@sequence.4">
    <questions text="Paciente tem algum sinal de alarme abaixo?" url="" id="10326">
      <answers xsi:type="pathwayMetamodel:Choice" name="dengue_sinais_alarme" id="19984" weight="1" value="0.0">
        <option description="Dor abdominal intensa" weight="1"/>
        <option description="V&#xf4;mitos persistentes" weight="1"/>
        <option description="Ac&#xfa;mulo de l&#xed;quidos (ascite, derrame pleural, derrame peric&#xe1;rdico)" weight="1"/>
        <option description="Aumento progressivo do hemat&#xf3;crito" weight="1"/>
        <option description="Hepatomegalia > 2cm abaixo do rebordo costal" weight="1"/>
        <option description="Letargia e/ou irritabilidade" weight="1"/>
        <option description="Sangramento de mucosas" weight="1"/>
        <option description="Hipotens&#xe3;o postural e/ou lipot&#xed;mia" weight="1"/>
      </answers>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20405" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="[Dengue] Verifica Sinais de Gravidade" description="" inputSequences="//@sequence.1" outputSequences="//@sequence.2 //@sequence.3">
    <questions text="Paciente tem algum sinal de gravidade abaixo?" url="" id="10327">
      <answers xsi:type="pathwayMetamodel:Choice" name="dengue_sinais_gravidade" id="19985" weight="1" value="0.0">
        <option description="Extravasamento grave de plasma, levando ao choque evidenciado por: taquicardia, extremidades distais frias, pulso fraco e filiforme, tempo enchimento capilar lento (>2seg), press&#xe3;o arterial convergente (&lt;20mmHg), taquipneia, olig&#xfa;ria, hipotens&#xe3;o, cianose" weight="1"/>
        <option description="Comprometimento grave de &#xf3;rg&#xe3;os" weight="1"/>
        <option description="Sangramento grave" weight="1"/>
      </answers>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment" id="20406" type="Tratamento" type_verbose="Tratamento" url="" name="[Dengue] Grupo D - Dengue Grave" description="&lt;p>&lt;strong>GRUPO D - DENGUE GRAVE&lt;br />&#xA;&lt;br />&#xA;Hidratar o paciente:&amp;nbsp;&lt;/strong>SF 0,9% 20ml/kg em 20 min(se paciente cardiopata, realizar SF 0,9% 10ml/kg). &lt;strong>Repetir at&amp;eacute; 3 vezes, se necess&amp;aacute;rio.&lt;/strong>&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>Reavaliar o paciente a cada 15-30 min.&amp;nbsp;&lt;/strong>&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>Controle rigoroso de SV (curva press&amp;oacute;rica). Monitoriza&amp;ccedil;&amp;atilde;o.&lt;/strong>&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>Sintom&amp;aacute;ticos:&lt;/strong>&amp;nbsp;Dipirona gotas ou Paracetamol gotas.&amp;nbsp;&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>Solicitar:&amp;nbsp;&lt;/strong>hemograma completo, TGO, TGP, TAP, TTPA, PTF,&amp;nbsp;s&amp;oacute;dio, pot&amp;aacute;ssio, ureia, creatinina, gasometria arterial, USG abdome (se queixa de dor abdominal). Solicitar pesquisa antigeno NS1 (se tiver nas primeiras 72h do quadro), Raio X T&amp;oacute;rax.&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>Passar caso com chefe de equipe e solicitar vaga de UTI&lt;/strong>&lt;/p>" isTerminal="true" inputSequences="//@sequence.2">

  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20289" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Calcular escala NIHSS" description="&lt;p>Calcular escala NIHSS:&lt;/p>" inputSequences="//@sequence.6" outputSequences="//@sequence.3">
    <questions text="1a. N&#xed;vel de Consci&#xea;ncia" url="" id="10267">
      <answers xsi:type="pathwayMetamodel:Choice" name="nivel_consciencia" id="19860" weight="1" value="0.0">
        <option description="Alerta; responde com entusiasmo."/>
        <option description="N&#xe3;o alerta, mas ao ser acordado por min&#xed;ma estimula&#xe7;&#xe3;o obedece, responde ou reage" weight="1"/>
        <option description="N&#xe3;o alerta, requer repetida estimula&#xe7;&#xe3;o ou estimula&#xe7;&#xe3;o dolorosa para realizar movimentos" weight="2"/>
        <option description="Responde somente com reflexo motor ou rea&#xe7;&#xf5;es auton&#xf4;micas, ou totalmente irresponsivo, fl&#xe1;cido e arreflexo." weight="3"/>
      </answers>
    </questions>
    <questions text="1b. Perguntas de N&#xed;vel de Consci&#xea;ncia" url="" id="10268">
      <answers xsi:type="pathwayMetamodel:Choice" name="p_nivel_consciencia" id="19881" weight="1" value="0.0">
        <option description="Responde ambas as quest&#xf5;es corretamente"/>
        <option description="Responde uma quest&#xe3;o corretamente" weight="1"/>
        <option description="N&#xe3;o responde nenhuma quest&#xe3;o corretamente" weight="2"/>
      </answers>
    </questions>
    <questions text="1c. Comandos de N&#xed;vel de Consci&#xea;ncia" url="" id="10269">
      <answers xsi:type="pathwayMetamodel:Choice" name="c_nivel_consciencia" id="19882" weight="1" value="0.0">
        <option description="Realiza ambas as tarefas corretamente"/>
        <option description="Realiza uma tarefa corretamente" weight="1"/>
        <option description="N&#xe3;o realiza nenhuma tarefa corretamente" weight="2"/>
      </answers>
    </questions>
    <questions text="2. Melhor olhar conjugado" url="" id="10270">
      <answers xsi:type="pathwayMetamodel:Choice" name="m_olhar_conjugado" id="19883" weight="1" value="0.0">
        <option description="Normal"/>
        <option description="Paralisia parcial do olhar (olhar anormal em um ou ambos os olhos, mas n&#xe3;o h&#xe1; desvio for&#xe7;ado ou paresia total do olhar)" weight="1"/>
        <option description="Desvio for&#xe7;ado ou paralisia total do olhar que n&#xe3;o podem ser vencidos pela manobra &#xf3;culo-cef&#xe1;lica" weight="2"/>
      </answers>
    </questions>
    <questions text="3. Campos visuais" url="" id="10271">
      <answers xsi:type="pathwayMetamodel:Choice" name="campos_visuais" id="19884" weight="1" value="0.0">
        <option description="Sem perda visual"/>
        <option description="Hemianopsia parcial" weight="1"/>
        <option description="Hemianopsia completa" weight="2"/>
        <option description="Hemianopsia bilateral (cego, incluindo cegueira cortical)" weight="3"/>
      </answers>
    </questions>
    <questions text="4. Paralisia Facial" url="" id="10272">
      <answers xsi:type="pathwayMetamodel:Choice" name="paralisia_facial" id="19885" weight="1" value="0.0">
        <option description="Movimentos normais sim&#xe9;tricos"/>
        <option description="Paralisia facial leve (apagamento de prega nasolabial, assimetria no sorriso)" weight="1"/>
        <option description="Paralisia facial central evidente (paralisia facial total ou quase toral da regi&#xe3;o interior da face)" weight="2"/>
        <option description="Paralisia facial completa (aus&#xea;ncia de movimentos faciais das regi&#xf5;es superior e inferior da face)" weight="3"/>
      </answers>
    </questions>
    <questions text="5. Membros superiores" url="" id="10273">
      <answers xsi:type="pathwayMetamodel:Choice" name="membros_superiores" id="19886" weight="1" value="0.0">
        <option description="Sem queda; mant&#xe9;m o bra&#xe7;o 90&#xba; (ou 45&#xba;) por 10 segundos completos"/>
        <option description="Queda; ment&#xe9;m o bra&#xe7;o a 90&#xba; (ou 45&#xba;), por&#xe9;m este apresenta queda antes dos 10 segundos completos; n&#xe3;o toca a cama ou outro suporte" weight="1"/>
        <option description="Algum esfor&#xe7;o contra a gravidade; o bra&#xe7;o n&#xe3;o atinge ou n&#xe3;o mant&#xe9;m 90&#xba; (ou 45&#xba;), cai na cama, mas tem alguma for&#xe7;a contra a gravidade" weight="2"/>
        <option description="Nenhum esfor&#xe7;o contra a gravidade; bra&#xe7;o despenca" weight="3"/>
        <option description="Nenhum movimento" weight="4"/>
      </answers>
    </questions>
    <questions text="6. Membros inferiores" url="" id="10274">
      <answers xsi:type="pathwayMetamodel:Choice" name="membros_inferiores" id="19887" weight="1" value="0.0">
        <option description="Sem queda; mant&#xe9;m a perna a 30&#xba; por 5 segundos completos"/>
        <option description="Queda; mant&#xe9;m a perna a 30&#xba;, por&#xe9;m esta apresenta queda antes dos 5 segundos completos; n&#xe3;o toca a cama ou outro suporte" weight="1"/>
        <option description="Algum esfor&#xe7;o contra a gravidade; a perna n&#xe3;o atinge ou n&#xe3;o mant&#xe9;m 30&#xba;, cai na cama, mas tem alguma for&#xe7;a contra a gravidade" weight="2"/>
        <option description="Nenhum esfor&#xe7;o contra a gravidade; perna despenca" weight="3"/>
        <option description="Nenhum movimento" weight="4"/>
      </answers>
    </questions>
    <questions text="7. Ataxia de membros" url="" id="10275">
      <answers xsi:type="pathwayMetamodel:Choice" name="ataxia_membros" id="19888" weight="1" value="0.0">
        <option description="Ausente"/>
        <option description="Presente em um membro" weight="1"/>
        <option description="Presente em dois membros" weight="2"/>
      </answers>
    </questions>
    <questions text="8. Sensibilidade" url="" id="10276">
      <answers xsi:type="pathwayMetamodel:Choice" name="sensibilidade" id="19889" weight="1" value="0.0">
        <option description="Normal; nenhuma perda"/>
        <option description="Perda sensitiva leve a moderada; a sensibilidade ao beliscar &#xe9; menos aguda ou diminu&#xed;da do lado afetado, ou h&#xe1; uma perda da dor superficial ao beliscar, mas o paciente est&#xe1; ciente de que est&#xe1; sendo tocado" weight="1"/>
        <option description="Perda da sensibilidade grave ou total; o paciente n&#xe3;o sente que est&#xe1;s sendo tocado" weight="2"/>
      </answers>
    </questions>
    <questions text="9. Melhor linguagem (afasia)" url="" id="10277">
      <answers xsi:type="pathwayMetamodel:Choice" name="linguagem_afasia" id="19890" weight="1" value="0.0">
        <option description="Sem afasia; normal"/>
        <option description="Afasia leve a moderada; alguma perda &#xf3;bvia da flu&#xea;ncia ou dificuldade de compreens&#xe3;o, sem limita&#xe7;&#xe3;o significativa das ideias express&#xe3;o ou forma de express&#xe3;o. ." weight="1"/>
        <option description="Afasia grave; toda a comunica&#xe7;&#xe3;o &#xe9; feita atrav&#xe9;s de express&#xf5;es fragmentadas; grande necessidade de interfer&#xea;ncia, questionamento e adivinha&#xe7;&#xe3;o por parte do ouvinte." weight="2"/>
        <option description="Mudo, afasia global; nenhuma fala &#xfa;til ou compreens&#xe3;o auditiva" weight="3"/>
      </answers>
    </questions>
    <questions text="10. Disartia" url="" id="10278">
      <answers xsi:type="pathwayMetamodel:Choice" name="disartia" id="19891" weight="1" value="0.0">
        <option description="Normal"/>
        <option description="Disartia leve a moderada; paciente arrasta pelo menos algumas palavras, e na pior das hip&#xf3;teses, pode ser entendido com alguma dificuldade." weight="1"/>
        <option description="Disartia grave; fala do paciente &#xe9; t&#xe3;o empastada que chega a ser inintelig&#xed;vel, na aus&#xea;ncia de difasia ou com difasia desproporcional, ou &#xe9; mudo/an&#xe1;rtrico" weight="2"/>
      </answers>
    </questions>
    <questions text="11. Extin&#xe7;&#xe3;o ou Desaten&#xe7;&#xe3;o (antiga neglig&#xea;ncia)" url="" id="10279">
      <answers xsi:type="pathwayMetamodel:Choice" name="extincao_desatencao" id="19892" weight="1" value="0.0">
        <option description="Nenhuma anormalidade"/>
        <option description="Desaten&#xe7;&#xe3;o visual, t&#xe1;til, auditiva, espacial ou pessoal, ou extin&#xe7;&#xe3;o &#xe0; estimula&#xe7;&#xe3;o simult&#xe2;nea em uma das modalidades sensoriais" weight="1"/>
        <option description="Profunda hemi-desaten&#xe7;&#xe3;o ou hemi-desaten&#xe7;&#xe3;o para mais de uma modalidade; n&#xe3;o reconhece a pr&#xf3;pria m&#xe3;o e se orienta somente para um lado do espa&#xe7;o" weight="2"/>
      </answers>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="20290" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Avaliar contraindica&#xe7;&#xf5;es" description="&lt;p>Contra-indica&amp;ccedil;&amp;otilde;es para tratamento Trombol&amp;iacute;tico:&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>In&amp;iacute;cio dos sintomas superior a 4,5h (se o paciente foi encontrado com d&amp;eacute;ficits ou acordou assim, considerar o &amp;uacute;ltimo hor&amp;aacute;rio em que foi visto bem);&lt;/li>&#xA;&#x9;&lt;li>Uso de Marevan com RNI &amp;gt; 1,7 ou heparina n&amp;atilde;o fracionada nas &amp;uacute;ltimas 48h com PTTA&amp;nbsp; elevado;&lt;/li>&#xA;&#x9;&lt;li>Uso de novos anticoagulantes (inibidores diretos da trombina ou do fator Xa) nas &amp;uacute;ltimas 48h;&lt;/li>&#xA;&#x9;&lt;li>Anticoagula&amp;ccedil;&amp;atilde;o plena com heparina de baixo peso molecular (Enoxaparina) nas &amp;uacute;ltimas 24h;&lt;/li>&#xA;&#x9;&lt;li>Plaquetas &amp;lt; 100000/mm3;&lt;/li>&#xA;&#x9;&lt;li>Hist&amp;oacute;ria pr&amp;eacute;via de hemorragia intracraniana, MAV ou neoplasia intracraniana intra-axial;&lt;/li>&#xA;&#x9;&lt;li>Pacientes que tiveram nos &amp;uacute;ltimos 3 meses: AVCi, TCE grave, cirurgia intracraniana ou intraespinhal, IAM;&lt;/li>&#xA;&#x9;&lt;li>Neoplasia maligna do TGI ou hemorragia do TGI/geniturin&amp;aacute;ria nos &amp;uacute;ltimos 21 dias;&lt;/li>&#xA;&#x9;&lt;li>Melhora r&amp;aacute;pida e completa dos d&amp;eacute;ficits no per&amp;iacute;odo que antecede a tromb&amp;oacute;lise;&lt;/li>&#xA;&#x9;&lt;li>Hipertens&amp;atilde;o (PAS &amp;gt;= 185 e/ou PAD &amp;gt;=110) refrat&amp;aacute;ria ao tratamento cl&amp;iacute;nico;&lt;/li>&#xA;&#x9;&lt;li>AVC com sintomas m&amp;iacute;nimos, sem repercuss&amp;atilde;o;&lt;/li>&#xA;&#x9;&lt;li>TC de cr&amp;acirc;nio com hipodensidade precoce &amp;gt; 1/3 do territ&amp;oacute;rio da ACM;&lt;/li>&#xA;&#x9;&lt;li>Glicemia &amp;lt; 50mg/dl com revers&amp;atilde;o dos sintomas ap&amp;oacute;s a corre&amp;ccedil;&amp;atilde;o;&lt;/li>&#xA;&#x9;&lt;li>Evid&amp;ecirc;ncia de endocardite ou &amp;ecirc;mbolo s&amp;eacute;ptico, gravidez;&lt;/li>&#xA;&#x9;&lt;li>Suspeita cl&amp;iacute;nica de hemorragia subaracnoide ou dissec&amp;ccedil;&amp;atilde;o aguda de aorta;&lt;/li>&#xA;&#x9;&lt;li>Cirurgia de grande porte, procedimento invasivo ou trauma grave nos &amp;uacute;ltimos 14 dias;&lt;/li>&#xA;&#x9;&lt;li>Pun&amp;ccedil;&amp;atilde;o lombar nos &amp;uacute;ltimos 7 dias;&lt;/li>&#xA;&#x9;&lt;li>Idade maior que 80 anos;&lt;/li>&#xA;&#x9;&lt;li>&#xA;&#x9;&lt;div>Pacientes usando anticoagulante, independente do INR;&lt;/div>&#xA;&#x9;&lt;/li>&#xA;&#x9;&lt;li>&#xA;&#x9;&lt;div>Antecedentes de AVE e DM associados;&lt;/div>&#xA;&#x9;&lt;/li>&#xA;&#x9;&lt;li>&#xA;&#x9;&lt;div>Tomografia computadorizada com evid&amp;ecirc;ncia de hemorragia.&lt;/div>&#xA;&#x9;&lt;/li>&#xA;&lt;/ul>" inputSequences="//@sequence.3" outputSequences="//@sequence.4 //@sequence.5">
    <questions text="Paciente possui uma ou mais contra-indica&#xe7;&#xe3;o para Tromb&#xf3;lise?" url="" id="10280">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="contra_indicacao_trombolise" id="19893" weight="1" value="false"/>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:Information" id="20291" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="Verificar tratamento cl&#xed;nico" description="&lt;p>&lt;span style=&quot;font-size:20px&quot;>Verificar tratamento cl&amp;iacute;nico a ser realizado junto com a chefia.&amp;nbsp;&lt;/span>&lt;/p>" isTerminal="true" inputSequences="//@sequence.4"/>
  <element xsi:type="pathwayMetamodel:Treatment" id="20292" type="Tratamento" type_verbose="Tratamento" url="" name="Tromb&#xf3;lise" description="&lt;p>&lt;strong>&lt;u>Intru&amp;ccedil;&amp;otilde;es para&amp;nbsp;INFUS&amp;Atilde;O DO TRATAMENTO TROMBOL&amp;Iacute;TICO (rTPA):&lt;/u>&lt;/strong>&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>Instalar o segundo acesso venoso calibroso no membro superior, se este n&amp;atilde;o tiver j&amp;aacute; confeccionado.&lt;/li>&#xA;&#x9;&lt;li>Dose de Alteplase: 0,9mg/kg (m&amp;aacute;ximo: 90mg): 10% da dose em 1 minuto e o restante em 1hora.&lt;/li>&#xA;&#x9;&lt;li>O acesso para a administra&amp;ccedil;&amp;atilde;o da Alteplase deve ser exclusivo e a infus&amp;atilde;o controlada por bomba de infus&amp;atilde;o cont&amp;iacute;nua.&lt;/li>&#xA;&#x9;&lt;li>N&amp;atilde;o utilizar antiagregante ou anticoagulante (mesmo em dose profil&amp;aacute;tica) nas pr&amp;oacute;ximas 24h seguindo a infus&amp;atilde;o.&lt;/li>&#xA;&#x9;&lt;li>N&amp;atilde;o realizar acesso arterial ou acesso venoso profundo nas pr&amp;oacute;ximas 24h seguindo a infus&amp;atilde;o.&lt;/li>&#xA;&#x9;&lt;li>Evitar sondagem vesical nas pr&amp;oacute;ximas 24h seguindo a infus&amp;atilde;o. Se extremamente necess&amp;aacute;rio, aguardar pelo menos 30 min do fim da infus&amp;atilde;o.&lt;/li>&#xA;&#x9;&lt;li>Evitar sondagem nasog&amp;aacute;strica/nasoenteral ou nasog&amp;aacute;stricas nas pr&amp;oacute;ximas 24h seguindo a infus&amp;atilde;o.&lt;/li>&#xA;&#x9;&lt;li>Manter o paciente em jejum por 24h devido o risco de hemorragia e necessidade de interven&amp;ccedil;&amp;atilde;o cir&amp;uacute;rgica de urg&amp;ecirc;ncia.&lt;/li>&#xA;&#x9;&lt;li>Manter hidrata&amp;ccedil;&amp;atilde;o com soro fisiol&amp;oacute;gico.&lt;/li>&#xA;&#x9;&lt;li>Solicitar TC de cr&amp;acirc;nio sem contraste para controle ap&amp;oacute;s 24h do fim da infus&amp;atilde;o.&lt;/li>&#xA;&#x9;&lt;li>Ap&amp;oacute;s 24h do tratamento trombol&amp;iacute;tico, o tratamento seguir&amp;aacute; as mesmas orienta&amp;ccedil;&amp;otilde;es destinadas aos pacientes que n&amp;atilde;o foram submetidos &amp;agrave; tromb&amp;oacute;lise (antiagrega&amp;ccedil;&amp;atilde;o, anticoagula&amp;ccedil;&amp;atilde;o, profilaxia medicamentosa para TVP &amp;ndash; quando indicados), desde que n&amp;atilde;o tenham ocorrido complica&amp;ccedil;&amp;otilde;es.&lt;/li>&#xA;&lt;/ul>&#xA;&#xA;&lt;p>&lt;strong>&lt;u>Dose:&lt;/u>&lt;/strong>&lt;/p>&#xA;&#xA;&lt;table border=&quot;1&quot; cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; style=&quot;width:500px&quot;>&#xA;&#x9;&lt;tbody>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>PESO&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>VOLUME BOLUS (mL)&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>VOLUME EM UMA HORA (mL)&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>40&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>3.6&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>32.4&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>45&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>4.05&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>36.45&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>50&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>4.5&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>40.5&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>55&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>4.95&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>44.55&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>60&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>5.4&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>48.6&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>65&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>5.85&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>52.65&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>70&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>6.3&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>56.7&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>75&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>6.75&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>60.75&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>80&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>7.2&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>64.8&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>85&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>7.65&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>68.85&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>90&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>8.1&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>72.9&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>95&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>8.55&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>76.95&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>100 ou mais&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>9&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td style=&quot;text-align:center&quot;>81&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&lt;/tbody>&#xA;&lt;/table>&#xA;&#xA;&lt;p>&amp;nbsp;&lt;/p>&#xA;&#xA;&lt;p>&lt;u>&lt;strong>Suspender Tromb&amp;oacute;lise se:&lt;/strong>&lt;/u>&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>Evid&amp;ecirc;ncias de sangramento;&lt;/li>&#xA;&#x9;&lt;li>Rebaixamento do n&amp;iacute;vel de consci&amp;ecirc;ncia;&lt;/li>&#xA;&#x9;&lt;li>Crise convulsiva;&lt;/li>&#xA;&#x9;&lt;li>PAS &amp;gt; 180 e/ou PAD &amp;gt; 105, ou eleva&amp;ccedil;&amp;atilde;o s&amp;uacute;bita da PA; V&amp;ocirc;mitos;&lt;/li>&#xA;&#x9;&lt;li>Eleva&amp;ccedil;&amp;atilde;o do NIHSS.&lt;/li>&#xA;&lt;/ul>" isTerminal="true" inputSequences="//@sequence.5"/>
  <element xsi:type="pathwayMetamodel:Information" id="20293" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="Calcular escala NIHSS" description="&lt;p>Instru&amp;ccedil;&amp;otilde;es:&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>1a. N&amp;iacute;vel de Consci&amp;ecirc;ncia: &lt;/strong>&lt;/u>O investigador deve escolher uma resposta&amp;nbsp;mesmo se uma avalia&amp;ccedil;&amp;atilde;o completa &amp;eacute;&amp;nbsp;prejudicada por obst&amp;aacute;culos como um tubo orotraqueal, barreiras de linguagem, trauma ou curativo orotraqueal. Um &lt;strong>3 &lt;/strong>&amp;eacute; dado apenas se o paciente n&amp;atilde;o faz nenhum movimento (outro al&amp;eacute;m de postura reflexa) em resposta &amp;agrave; estimula&amp;ccedil;&amp;atilde;o dolorosa.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>1b. Perguntas de N&amp;iacute;vel de Consci&amp;ecirc;ncia:&lt;/strong>&lt;/u> O paciente &amp;eacute; questionado sobre o m&amp;ecirc;s e sua idade. A resposta deve ser correta &amp;ndash; n&amp;atilde;o h&amp;aacute; nota parcial por chegar perto. Pacientes com afasia ou estupor que n&amp;atilde;o compreendem as perguntas ir&amp;atilde;o receber&amp;nbsp;2. Pacientes incapazes de falar por tubo ou traumatismo orotraqueal, disartria grave de qualquer causa, barreiras de linguagem ou qualquer outro problema n&amp;atilde;o secund&amp;aacute;rio a afasia receber&amp;atilde;o 1. &amp;Eacute; importante considerar apenas a resposta inicial e que o examinador n&amp;atilde;o &amp;ldquo;ajude&amp;rdquo; o paciente com dicas verbais ou n&amp;atilde;o verbais.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>1c. Comandos de N&amp;iacute;vel de Consci&amp;ecirc;ncia:&lt;/strong>&lt;/u> O paciente &amp;eacute; solicitado a abrir e fechar os olhos e depois abrir e fechar a m&amp;atilde;o n&amp;atilde;o par&amp;eacute;tica. Substitua por outro comando de um &amp;uacute;nico passo se as m&amp;atilde;os n&amp;atilde;o puderem ser utilizadas. Devemos valorizar uma tentativa inequ&amp;iacute;voca, ainda que n&amp;atilde;o completada devido &amp;agrave; fraqueza muscular. Se o paciente n&amp;atilde;o responde &amp;agrave; ordem, a tarefa deve ser demonstrada usando gestos e o resultado registado. Aos pacientes com trauma, amputa&amp;ccedil;&amp;atilde;o ou outro impedimento f&amp;iacute;sico devem ser dadas ordens simples adequadas. Pontue s&amp;oacute; a primeira tentativa.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>2. Melhor Olhar Conjugado: &lt;/strong>&lt;/u>Teste apenas os movimentos oculares horizontais. Os movimentos oculares volunt&amp;aacute;rios ou reflexos (oculocef&amp;aacute;lico) s&amp;atilde;o pontuados, mas a prova cal&amp;oacute;rica n&amp;atilde;o &amp;eacute; avaliada. Se o paciente tem um desvio conjugado do olhar, que &amp;eacute; revertido pela atividade volunt&amp;aacute;ria ou reflexa, a pontua&amp;ccedil;&amp;atilde;o ser&amp;aacute; 1. Se o paciente tem uma par&amp;eacute;sia de nervo perif&amp;eacute;rico isolada (NC III, IV ou VI), pontue 1. O olhar &amp;eacute; testado em todos os pacientes af&amp;aacute;sicos. Os pacientes com trauma ou curativo ocular, cegueira pr&amp;eacute;-existente ou outro dist&amp;uacute;rbio de acuidade ou campo visual devem ser testados com movimentos reflexos e a escolha feita pelo examinador. Estabelecer contacto visual e mover-se perto do paciente de um lado para outro pode esclarecer a presen&amp;ccedil;a de paralisia do olhar conjugado.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>3. Campos visuais:&lt;/strong>&lt;/u> Os campos visuais (quadrantes superiores e inferiores) s&amp;atilde;o testados por confronta&amp;ccedil;&amp;atilde;o, utilizando contagem de dedos ou amea&amp;ccedil;a visual, conforme apropriado. O paciente pode ser encorajado, mas basta identificar olhando para o lado em que mexem os dedos para ser considerado como normal. Se houver cegueira unilateral ou enuclea&amp;ccedil;&amp;atilde;o, os campos visuais no olho restante s&amp;atilde;o avaliados. Pontue 1 apenas se houver uma assimetria clara, incluindo quadrantan&amp;oacute;psia. Se o paciente &amp;eacute; cego por qualquer causa, pontue 3. A estimula&amp;ccedil;&amp;atilde;o dupla simult&amp;acirc;nea &amp;eacute; realizada neste momento. Se houver extin&amp;ccedil;&amp;atilde;o, o paciente recebe 1 e os resultados s&amp;atilde;o usados para responder a quest&amp;atilde;o 11.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>4. Paralisia&amp;nbsp;Facial:&lt;/strong>&lt;/u> Pergunte ou use gestos para encorajar o paciente a mostrar os dentes ou levantar as sobrancelhas e fechar com for&amp;ccedil;a os olhos. Pontue a simetria da contrac&amp;ccedil;&amp;atilde;o facial em resposta ao est&amp;iacute;mulo doloroso nos pacientes pouco responsivos ou que n&amp;atilde;o compreendam. Na presen&amp;ccedil;a de traumatismo, tubo orotraqueal, adesivos ou outra barreira f&amp;iacute;sica que possam esconder a face, estes devem ser removidos, tanto quanto poss&amp;iacute;vel.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>5. Membros Superiores:&lt;/strong>&lt;/u> O bra&amp;ccedil;o &amp;eacute; colocado na posi&amp;ccedil;&amp;atilde;o apropriada: extens&amp;atilde;o dos bra&amp;ccedil;os, palmas para baixo, a 90&amp;ordm; se sentado ou a 45&amp;ordm; se posi&amp;ccedil;&amp;atilde;o supina. Pontue-se a queda do bra&amp;ccedil;o quando esta ocorre antes de 10 segundos. O paciente af&amp;aacute;sico &amp;eacute; encorajado atrav&amp;eacute;s de firmeza na voz ou gestos, mas n&amp;atilde;o com estimula&amp;ccedil;&amp;atilde;o dolorosa. Cada membro &amp;eacute; testado isoladamente, come&amp;ccedil;ando no bra&amp;ccedil;o n&amp;atilde;o-par&amp;eacute;tico.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>6. Membros Inferiores:&lt;/strong>&lt;/u> A perna &amp;eacute; colocada na posi&amp;ccedil;&amp;atilde;o apropriada: extens&amp;atilde;o a 30&amp;ordm;. Teste sempre na posi&amp;ccedil;&amp;atilde;o supina. Pontue-se a queda da perna quando esta ocorre antes de 5 segundos. O paciente af&amp;aacute;sico &amp;eacute; encorajado atrav&amp;eacute;s de firmeza na voz ou gestos, mas n&amp;atilde;o com estimula&amp;ccedil;&amp;atilde;o dolorosa. Cada membro &amp;eacute; testado isoladamente, come&amp;ccedil;ando na perna n&amp;atilde;o-par&amp;eacute;tica.&lt;/li>&#xA;&#x9;&lt;li>&lt;strong>&lt;u>7. Ataxia de membros:&lt;/u>&lt;/strong> Este item procura evid&amp;ecirc;ncia de les&amp;atilde;o cerebelosa unilateral. Teste com os olhos abertos. No caso de d&amp;eacute;fice de campo visual, assegure-se que o teste &amp;eacute; feito no campo visual intacto. Os testes dedo-nariz e calcanhar-joelho s&amp;atilde;o realizados em ambos os lados e a ataxia &amp;eacute; valorizada, apenas, se for desproporcional em rela&amp;ccedil;&amp;atilde;o &amp;agrave; fraqueza muscular. A ataxia &amp;eacute; considerada ausente no doente com perturba&amp;ccedil;&amp;atilde;o da compreens&amp;atilde;o ou pl&amp;eacute;gico.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>8. Sensibilidade: &lt;/strong>&lt;/u>Avalie a sensibilidade ou m&amp;iacute;mica facial &amp;agrave; picada de alfinete ou a resposta de retirada ao est&amp;iacute;mulo doloroso em paciente obnubilado ou af&amp;aacute;sico. S&amp;oacute; a perda de sensibilidade atribu&amp;iacute;da ao AVC &amp;eacute; pontuada. Teste tantas as partes do corpo &amp;ndash; membros superiores (excepto m&amp;atilde;os), inferiores (excepto p&amp;eacute;s), tron11. Extin&amp;ccedil;&amp;atilde;o e Desaten&amp;ccedil;&amp;atilde;o, antiga neglig&amp;ecirc;ncia. A informa&amp;ccedil;&amp;atilde;o suficiente para a identifica&amp;ccedil;&amp;atilde;o de neglig&amp;ecirc;ncia pode ter sido obtida durante os testes anteriores. Se o doente tem perda visual grave, que impede o teste da estimula&amp;ccedil;&amp;atilde;o visual dupla simult&amp;acirc;nea, e os est&amp;iacute;mulos cut&amp;acirc;neos s&amp;atilde;o normais, a pontua&amp;ccedil;&amp;atilde;o &amp;eacute; normal. Se o doente tem afasia, mas parece identificar ambos os lados, &amp;eacute; pontuado como normal. A presen&amp;ccedil;a de neglig&amp;ecirc;ncia visuoespacial ou anosagnosia contribuem tamb&amp;eacute;m para a evid&amp;ecirc;ncia de anormalidade. Como a anormalidade s&amp;oacute; &amp;eacute; pontuada se presente, o item nunca &amp;eacute; considerado n&amp;atilde;o test&amp;aacute;vel.co e face &amp;ndash; quantas as necess&amp;aacute;rias para avaliar com precis&amp;atilde;o uma perda hemissensitiva. Pontue com 2 s&amp;oacute; se uma perda grave ou total da sensibilidade puder ser claramente demonstrada. Deste modo, doentes estuporosos ou af&amp;aacute;sicos ir&amp;atilde;o ser pontuados possivelmente com 1 ou 0. O doente com AVC do tronco cerebral com perda de sensibilidade bilateral &amp;eacute; pontuado com 2. Se o paciente n&amp;atilde;o responde e est&amp;aacute; quadripl&amp;eacute;gico, pontue 2. Pacientes em coma (item 1a=3) s&amp;atilde;o pontuados arbitrariamente com 2 neste item.&lt;/li>&#xA;&#x9;&lt;li>&lt;u>&lt;strong>9. Melhor linguagem:&lt;/strong>&lt;/u> Durante a pontua&amp;ccedil;&amp;atilde;o dos itens precedentes obter&amp;aacute; muita informa&amp;ccedil;&amp;atilde;o acerca da capacidade de compreens&amp;atilde;o. Pede-se ao doente para descrever o que est&amp;aacute; a acontecer na imagem em anexo, para nomear objectos num cart&amp;atilde;o de nomea&amp;ccedil;&amp;atilde;o anexo e para ler uma lista de frases em anexo. A compreens&amp;atilde;o &amp;eacute; julgada a partir destas respostas, assim como as referentes &amp;agrave;s ordens dadas no exame neurol&amp;oacute;gico geral precedente. Se a perda visual interferir com os testes, pe&amp;ccedil;a ao doente para identificar objetos colocados na m&amp;atilde;o, repetir frases e produzir discurso. O paciente entubado deve escrever as respostas. O doente em coma (1a=3) ser&amp;aacute; pontuado arbitrariamente com 3. O examinador deve escolher a pontua&amp;ccedil;&amp;atilde;o no doente com estupor ou pouco colaborante, mas a pontua&amp;ccedil;&amp;atilde;o de 3 est&amp;aacute; reservada a doentes em mutismo e que n&amp;atilde;o cumpram nenhuma ordem simples.&lt;/li>&#xA;&#x9;&lt;li>&lt;strong>&lt;u>10. Disartria:&lt;/u>&lt;/strong> Se acredita que o doente consegue, pede-se para ler ou repetir as palavras da lista anexa. Se o paciente tem afasia grave, a clareza da articula&amp;ccedil;&amp;atilde;o da fala espont&amp;acirc;nea pode ser pontuada.&amp;nbsp;&lt;strong>N&amp;atilde;o diga ao paciente a raz&amp;atilde;o pela qual est&amp;aacute; a ser testado.&lt;/strong>&lt;/li>&#xA;&#x9;&lt;li>&lt;strong>&lt;u>11. Extin&amp;ccedil;&amp;atilde;o e Desaten&amp;ccedil;&amp;atilde;o, antiga neglig&amp;ecirc;ncia:&lt;/u>&lt;/strong>&amp;nbsp;A informa&amp;ccedil;&amp;atilde;o suficiente para a identifica&amp;ccedil;&amp;atilde;o de neglig&amp;ecirc;ncia pode ter sido obtida durante os testes anteriores. Se o doente tem perda visual grave, que impede o teste da estimula&amp;ccedil;&amp;atilde;o visual dupla simult&amp;acirc;nea, e os est&amp;iacute;mulos cut&amp;acirc;neos s&amp;atilde;o normais, a pontua&amp;ccedil;&amp;atilde;o &amp;eacute; normal. Se o doente tem afasia, mas parece identificar ambos os lados, &amp;eacute; pontuado como normal. A presen&amp;ccedil;a de neglig&amp;ecirc;ncia visuoespacial ou anosagnosia contribuem tamb&amp;eacute;m para a evid&amp;ecirc;ncia de anormalidade. Como a anormalidade s&amp;oacute; &amp;eacute; pontuada se presente, o item nunca &amp;eacute; considerado n&amp;atilde;o test&amp;aacute;vel.&lt;/li>&#xA;&lt;/ul>" inputSequences="//@sequence.2" outputSequences="//@sequence.6"/>
  <sequence inputStep="//@element.1" outputStep="//@element.0" id="20294" type="Sequencia" type_verbose="Sequencia" url="" name="S1 (tempo > 4,5h)" description="tempo > 4,5h">

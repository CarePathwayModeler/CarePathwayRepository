      <operand xsi:type="pathwayMetamodel:Choice" name="hora_inicio" id="19859" weight="1" value="0.0">
        <option description="&lt; 4,5 horas" weight="1"/>
        <option description="> 4,5 horas" weight="2"/>
      </operand>
      <operand xsi:type="pathwayMetamodel:Numeric" name="2" id="19894" weight="1" value="2.0"/>
    </operation>
  </sequence>
  <sequence inputStep="//@element.2" outputStep="//@element.0" id="20295" type="Sequencia" type_verbose="Sequencia" url="" name="S2 (tempo &lt; 4,5h)" description="tempo &lt; 4,5h">
    <operation name="tempo &lt; 4,5h" Operator="equal" id="19914">
      <operand xsi:type="pathwayMetamodel:Choice" name="hora_inicio" id="19859" weight="1" value="0.0">
        <option description="&lt; 4,5 horas" weight="1"/>
        <option description="> 4,5 horas" weight="2"/>
      </operand>
      <operand xsi:type="pathwayMetamodel:Numeric" name="1" id="19895" weight="1" value="1.0"/>
    </operation>

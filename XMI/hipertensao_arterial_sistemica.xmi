  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="17557" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Classificar Hipertens&#xe3;o" description="" inputSequences="//@sequence.0" outputSequences="//@sequence.2 //@sequence.3 //@sequence.4">
    <questions text="Emerg&#xea;ncia Hipertensiva: Paciente com les&#xe3;o aguda de &#xf3;rg&#xe3;os-alvo e risco iminente de morte. Ex: encefalopatia hipertensiva, AVC, dissec&#xe7;&#xe3;o aorta, edema agudo de pulm&#xe3;o, s&#xed;ndrome coronariana aguda, crise de feocromocitoma, pr&#xe9;-ecl&#xe2;mpsia." url="" id="9123">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="emergencia_hipertensiva" id="17421" weight="1" value="false"/>
    </questions>
    <questions text="Urg&#xea;ncia Hipertensiva: Paciente com risco potencial de les&#xe3;o aguda de &#xf3;rg&#xe3;os-alvo. Ex: com alguma les&#xe3;o pr&#xe9;via de &#xf3;rg&#xe3;o-alvo (IAM, AVC ou ICC) apresentando, portanto, maior risco de novas complica&#xe7;&#xf5;es em curto prazo na vig&#xea;ncia de PA acentuadamente alta." url="" id="9124">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="urgencia_hipertensiva" id="17422" weight="1" value="false"/>
    </questions>
    <questions text="Pseudocrise Hipertensiva: O aumento da PA &#xe9; desencadeado por dor (c&#xf3;licas, cefaleias, fibromialgia), ansiedade e/ou abandono do tratamento anti-hipertensivo." url="" id="9125">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="pseudocrise_hipertensiva" id="17423" weight="1" value="false"/>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment" id="17558" type="Tratamento" type_verbose="Tratamento" url="" name="Emerg&#xea;ncia Hipertensiva" description="&lt;p>&lt;span style=&quot;font-size:14px&quot;>Discutir caso com l&amp;iacute;der local&lt;br />&#xA;Encaminhar para sala de cuidados, iniciar anti-hipertensivo parenteral, diferenciar e tratar conforme a causa da emerg&amp;ecirc;ncia hipertensiva e internamento em UTI&lt;/span>&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>&lt;span style=&quot;font-size:14px&quot;>&lt;strong>Se suspeita de AVC&lt;/strong>, solicitar tomografia de cr&amp;acirc;nio sem contraste, avalia&amp;ccedil;&amp;atilde;o da neurologia. Diminuir em 15% a PA nas primeiras 24h, alcan&amp;ccedil;ando 160x100 mmHg (evitar PA inferior a este valor)&lt;/span>&lt;/li>&#xA;&#x9;&lt;li>&lt;span style=&quot;font-size:14px&quot;>&lt;strong>Se suspeita de pr&amp;eacute;-ecl&amp;acirc;mpsia&lt;/strong>, solicitar tamb&amp;eacute;m LDH, TGO, TGP, bilirrubinas&amp;nbsp;&lt;/span>&lt;/li>&#xA;&lt;/ul>" isTerminal="true" inputSequences="//@sequence.2 //@sequence.5">
    <examination url="ELETROCARDIOGRAMA" exam_id="20010010" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="HEMOGRAMA COMPLETO COM REVISAO DE LAMINAS" exam_id="28040481" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="UREIA" exam_id="28011414" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="CREATININA  (SANGUE)" exam_id="28010540" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="SODIO (SANGUE)" exam_id="28011279" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="POTASSIO" exam_id="28011210" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="TROPONINA" exam_id="28011783" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
    <examination url="RX TORAX P.A" exam_id="32050038" limb_side="" quantity="1" reason="Emerg&#xea;ncia hipertensiva" clinical_indication="n/a"/>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment" id="17559" type="Tratamento" type_verbose="Tratamento" url="" name="Urg&#xea;ncia Hipertensiva" description="&lt;p>&lt;span style=&quot;font-size:14px&quot;>Avaliar solicita&amp;ccedil;&amp;atilde;o de exames complementares conforme a comorbidade pr&amp;eacute;via do paciente e sintomas atuais associados.&amp;nbsp;&lt;br />&#xA;&lt;br />&#xA;Realizar tratamento anti-hipertensivo via oral com captopril ou clonidina (se disfun&amp;ccedil;&amp;atilde;o renal). Reavaliar PA ap&amp;oacute;s 1 hora, com objetivo PA &amp;lt;= 160x100&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:14px&quot;>Caso paciente apresente disfun&amp;ccedil;&amp;atilde;o renal, discutir caso com chefe local para prescri&amp;ccedil;&amp;atilde;o de Clonidina.&lt;br />&#xA;&lt;br />&#xA;Se paciente assintom&amp;aacute;tico, est&amp;aacute;vel, ajustar prescri&amp;ccedil;&amp;atilde;o de anti-hipertensivos de uso em domic&amp;iacute;lio, marcar consulta ambulatorial com cardiologista e procurar a emerg&amp;ecirc;ncia caso nova crise hipertensiva&lt;/span>&lt;/p>" inputSequences="//@sequence.3" outputSequences="//@sequence.11">
    <medication url="" med_id="36560" id="9070" name="CAPOTEN 25 MG COMP 1 UD" brand="" unit="UD" access="1" daily_dosage="1" cycles="1" treatement_time="9070" standard="null"/>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment" id="17560" type="Tratamento" type_verbose="Tratamento" url="" name="Pseudocrise Hipertensiva" description="&lt;p>&lt;span style=&quot;font-size:14px&quot;>&lt;strong>Tratar a causa de base do aumento da PA&lt;/strong>&lt;br />&#xA;&lt;br />&#xA;Realizar nova avalia&amp;ccedil;&amp;atilde;o da PA ap&amp;oacute;s 1 hora&lt;br />&#xA;&lt;br />&#xA;Se paciente assintom&amp;aacute;tico, est&amp;aacute;vel, marcar consulta ambulatorial com cl&amp;iacute;nico geral para pesquisa de diagn&amp;oacute;sticos diferenciais ou procurar cardiologista em caso de abandono do tratamento ou otimiza&amp;ccedil;&amp;atilde;o terap&amp;ecirc;utica nos pacientes hipertensos pr&amp;eacute;vios&lt;/span>&lt;/p>" inputSequences="//@sequence.4" outputSequences="//@sequence.12">
    <medication url="" med_id="38075" id="9071" name="DIAZEPAN 10 MG AMPL 2 ML" brand="" unit="AL" access="6" daily_dosage="1" cycles="1" treatement_time="9071" standard="null"/>
    <medication url="" med_id="38008" id="9072" name="DIPIRONA 1 G AMPL 2 ML" brand="" unit="AL" access="6" daily_dosage="1" cycles="1" treatement_time="9072" standard="null"/>
    <medication url="" med_id="59609" id="9073" name="PROFENID 100MG IV FRAP 1 UD" brand="" unit="AL" access="6" daily_dosage="1" cycles="1" treatement_time="9073" standard="null"/>
    <medication url="" med_id="39586" id="9074" name="TRAMAL 100MG AMPL 2 ML" brand="" unit="AL" access="6" daily_dosage="1" cycles="1" treatement_time="9074" standard="null"/>
    <medication url="" med_id="38121" id="9075" name="DIAZEPAN 5 MG COMP 1 UD" brand="" unit="UD" access="1" daily_dosage="1" cycles="1" treatement_time="9075" standard="null"/>
  </element>

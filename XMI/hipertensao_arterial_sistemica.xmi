  <element xsi:type="pathwayMetamodel:Information" id="17663" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="Mudan&#xe7;a de Estilo de Vida + Cardiologista" description="&lt;h3>&lt;span style=&quot;font-size:14px&quot;>Orienta&amp;ccedil;&amp;otilde;es quanto a mudan&amp;ccedil;a de estilo de vida (perda de peso, abandono do tabagismo, alimenta&amp;ccedil;&amp;atilde;o hiposs&amp;oacute;dica)&lt;br />&#xA;&lt;br />&#xA;&lt;strong>Agendar consulta ambulatorial com cardiologista.&lt;/strong>&lt;/span>&lt;/h3>" isTerminal="true" inputSequences="//@sequence.9"/>
  <element xsi:type="pathwayMetamodel:Information" id="17664" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="Ajuste da Terapia Hipertensiva" description="&lt;p>Realizar ajuste de terapia anti-hipertensiva e orienta&amp;ccedil;&amp;otilde;es de mudan&amp;ccedil;a de vida&lt;br />&#xA;&lt;br />&#xA;Agendar consulta ambulatorial com cardiologista.&lt;/p>" isTerminal="true" inputSequences="//@sequence.10"/>

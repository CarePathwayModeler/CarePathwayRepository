  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="17556" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Apresenta Les&#xe3;o?" description="" inputSequences="//@sequence.1" outputSequences="//@sequence.5 //@sequence.6">

  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="19145" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Avaliar gravidade" description="" inputSequences="//@sequence.11" outputSequences="//@sequence.12 //@sequence.13 //@sequence.14 //@sequence.15">
    <questions text="N&#xe3;o infectado: Ferida sem purul&#xea;ncia ou qualquer manifesta&#xe7;&#xe3;o de inflama&#xe7;&#xe3;o." url="" id="9823">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="gravidade_nao_infectado" id="18859" weight="1" value="false"/>
    </questions>
    <questions text="Infec&#xe7;&#xe3;o leve: Purul&#xea;ncia ou eritema, dor, calor ou endurecimento, mas qualquer celulite/eritema se estende &lt;= 2 cm ao redor da &#xfa;lcera e infec&#xe7;&#xe3;o &#xe9; limitada &#xe0; pele ou tecidos subcut&#xe2;neos superficiais. N&#xe3;o h&#xe1; outra complica&#xe7;&#xe3;o local ou doen&#xe7;a sist&#xea;mica." url="" id="9824">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="gravidade_infeccao_leve" id="18860" weight="1" value="false"/>
    </questions>
    <questions text="Infec&#xe7;&#xe3;o moderada: hemodinamicamente est&#xe1;vel, com (pelo menos 1): celulite com extens&#xe3;o > 2 cm, estria linfang&#xed;tica, dissemina&#xe7;&#xe3;o abaixo da f&#xe1;scia superficial, abscesso de tecido profundo, gangrena, envolvimento de m&#xfa;sculo, tend&#xe3;o, articula&#xe7;&#xe3;o ou osso." url="" id="9825">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="gravidade_infeccao_moderada" id="18921" weight="1" value="false"/>
    </questions>
    <questions text="Infec&#xe7;&#xe3;o grave: infec&#xe7;&#xe3;o em um paciente com toxicidade sist&#xea;mica ou instabilidade metab&#xf3;lica (febre, calafrios, taquicardia, hipotens&#xe3;o, confus&#xe3;o, v&#xf4;mitos, leucocitose, acidose, hiperglicemia grave ou azotemia)." url="" id="9826">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="gravidade_infeccao_grave" id="18922" weight="1" value="false"/>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:Information" id="19146" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="[N&#xe3;o infectado] Conduta" description="&lt;p>&lt;span style=&quot;font-size:16px&quot;>Avaliar necessidade de desbridamento. Se necess&amp;aacute;rio, encaminhar para o cirurgi&amp;atilde;o da emerg&amp;ecirc;ncia&lt;br />&#xA;&lt;br />&#xA;Em casos de &amp;uacute;lceras cr&amp;ocirc;nicas, n&amp;atilde;o infectadas, sem indica&amp;ccedil;&amp;atilde;o de desbridamento, acompanhamento ambulatorial com cirurgia vascular&lt;br />&#xA;&lt;br />&#xA;Na aus&amp;ecirc;ncia de pulsos, solicitar ultrassom com doppler arterial e venoso do membro acometido para avaliar causa isqu&amp;ecirc;mica&lt;/span>&lt;/p>" isTerminal="true" inputSequences="//@sequence.12"/>

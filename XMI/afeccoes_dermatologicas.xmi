  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="19077" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Avalia&#xe7;&#xe3;o do Quadro" description="&lt;p>Anamnese, sinais vitais, avalia&amp;ccedil;&amp;atilde;o da &amp;aacute;rea acometida, poss&amp;iacute;vel porta de entrada, acometimento agudo ou cr&amp;ocirc;nico.&lt;/p>" inputSequences="//@sequence.0" outputSequences="//@sequence.3 //@sequence.4 //@sequence.5 //@sequence.8 //@sequence.9 //@sequence.10 //@sequence.18 //@sequence.19 //@sequence.20 //@sequence.24">
    <questions text="HERPES LABIAL: les&#xf5;es no in&#xed;cio vesiculares, coalescentes, que evoluem para crostas." url="" id="9789">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="herpes_labial" id="18845" weight="1" value="false"/>
    </questions>
    <questions text="CELULITE: Infec&#xe7;&#xe3;o da derme profunda e tecido celular subcut&#xe2;neo, com caracter&#xed;stica mais infiltrativa e mal delimitada. Quadro mais arrastado." url="" id="9790">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="celulite" id="18846" weight="1" value="false"/>
    </questions>
    <questions text="IMPETIGO: Les&#xf5;es indolores eritematosas, acompanhadas de les&#xf5;es pustulares ou bolhosas." url="" id="9791">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="impetigo" id="18847" weight="1" value="false"/>
    </questions>
    <questions text="FURUNCULOS: N&#xf3;dulos inflamat&#xf3;rios, dolorosos e profundos com desenvolvimento oriundo da foliculite, que evoluem para drenagem espont&#xe2;nea de material purulento." url="" id="9792">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="furunculos" id="18848" weight="1" value="false"/>
    </questions>
    <questions text="P&#xc9; DIAB&#xc9;TICO: Complica&#xe7;&#xe3;o infecciosa decorrente de les&#xe3;o em membros inferiores. H&#xe1; eritema, calor, sensibilidade, edema, endurecimento e secre&#xe7;&#xf5;es purulentas." url="" id="9793">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="pe_diabetico" id="18849" weight="1" value="false"/>
    </questions>
    <questions text="URTIC&#xc1;RIA: Les&#xf5;es cut&#xe2;neas eritematosas, edemaciadas e pruriginosas de aparecimento abrupto, que surgem ap&#xf3;s contato com al&#xe9;rgenos." url="" id="9794">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="urticaria" id="18850" weight="1" value="false"/>
    </questions>
    <questions text="ERISIPELA: Infec&#xe7;&#xe3;o da derme, epiderme e vasos linf&#xe1;ticos superficiais. Dor, hiperemia, edema e aumento da temperatura local. Les&#xf5;es elevadas com clara diferencia&#xe7;&#xe3;o entre &#xe1;rea acometida e pele de aspecto normal. Mais comum em membros inferiores e face." url="" id="9795">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="erisipela" id="18851" weight="1" value="false"/>
    </questions>
    <questions text="ESCABIOSE: P&#xe1;pulas eritematosas com prurido que piora &#xe0; noite. Localizado preferencialmente em dedos das m&#xe3;os e p&#xe9;s, punhos, cotovelos, axilas, em torno dos mamilos, p&#xea;nis e bolsa escrotal." url="" id="9796">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="escabiose" id="18852" weight="1" value="false"/>
    </questions>
    <questions text="QUEIMADURAS" url="" id="9797">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="queimaduras" id="18853" weight="1" value="false"/>
    </questions>
    <questions text="HERPES ZOSTER: m&#xfa;ltiplas ves&#xed;culas, seguindo um derm&#xe1;tomo, com dor ou parestesia local." url="" id="9798">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="herpes_zoster" id="18854" weight="1" value="false"/>
    </questions>
  </element>
